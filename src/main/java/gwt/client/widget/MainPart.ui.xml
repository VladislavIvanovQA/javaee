<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:elem='urn:import:com.google.gwt.dom.client'>
    <ui:with field='res' type='gwt.client.text.ApplicationConstants'/>
    <ui:style>
        .redButton {
            font-size: 10px;
        }
        .marginTop5px {
            margin-top: 5px;
        }
    </ui:style>
    <g:VerticalPanel>
        <g:HorizontalPanel ui:field="infoPanelUSD" addStyleNames="{style.marginTop5px}">
            <g:Label ui:field="textUSD" text="defaultTest" addStyleNames="currency"/>
            <g:Label ui:field="valueUSD" text="defaultValue" addStyleNames="currency"/>
        </g:HorizontalPanel>
        <g:HorizontalPanel ui:field="infoPanelEUR" addStyleNames="{style.marginTop5px}">
            <g:Label ui:field="textEUR" text="defaultTest" addStyleNames="currency"/>
            <g:Label ui:field="valueEUR" text="defaultValue" addStyleNames="currency"/>
        </g:HorizontalPanel>
        <g:HorizontalPanel ui:field="infoPanelCNY" addStyleNames="{style.marginTop5px}">
            <g:Label ui:field="textCNY" text="defaultTest" addStyleNames="currency"/>
            <g:Label ui:field="valueCNY" text="defaultValue" addStyleNames="currency"/>
        </g:HorizontalPanel>
        <g:HorizontalPanel ui:field="infoPanelJPY" addStyleNames="{style.marginTop5px}">
            <g:Label ui:field="textJPY" text="defaultTest" addStyleNames="currency"/>
            <g:Label ui:field="valueJPY" text="defaultValue" addStyleNames="currency"/>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>