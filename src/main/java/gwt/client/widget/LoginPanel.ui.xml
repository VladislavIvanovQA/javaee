<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:elem='urn:import:com.google.gwt.dom.client'>
    <ui:with field='res' type='gwt.client.text.ApplicationConstants'/>
    <ui:style>
        .blackText {
            font-family: Arial, Sans-serif;
            color: #000000;
            font-size: 11px;
            text-align: left;
        }
    </ui:style>
    <g:VerticalPanel>
        <g:FlowPanel>
            <g:FlowPanel>
                <g:HorizontalPanel ui:field="loginpanel">
                    <g:Label text="Логин :"></g:Label>
                    <g:TextBox ui:field="login"></g:TextBox>
                </g:HorizontalPanel>
                <g:HorizontalPanel  ui:field="passwordpanel">
                    <g:Label text="Пароль :"></g:Label>
                    <g:PasswordTextBox ui:field="password"></g:PasswordTextBox>
                </g:HorizontalPanel>
                <g:HorizontalPanel>
                    <g:FlowPanel>
                        <g:Button ui:field="loginsubmit" text="Войти"></g:Button>
                    </g:FlowPanel>
                </g:HorizontalPanel>
                <g:Label ui:field = "completionLabel1" addStyleNames="{style.blackText}" />
                <g:Label ui:field = "completionLabel2" addStyleNames="{style.blackText}" />
            </g:FlowPanel>
        </g:FlowPanel>
    </g:VerticalPanel>
</ui:UiBinder>